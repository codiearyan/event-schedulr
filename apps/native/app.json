{
  "expo": {
    "name": "EventSchedulr",
    "slug": "event-schedulr",
    "version": "1.0.0",
    "scheme": "eventschedulr",
    "userInterfaceStyle": "automatic",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/logo.png",
      "backgroundColor": "#0a0a0f",
      "resizeMode": "contain"
    },
    "web": {
      "bundler": "metro"
    },
    "ios": {
      "bundleIdentifier": "com.eventschedulr.app",
      "supportsTablet": false,
      "splash": {
        "image": "./assets/images/logo.png",
        "backgroundColor": "#0a0a0f",
        "resizeMode": "contain"
      }
    },
    "android": {
      "package": "com.eventschedulr.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#0a0a0f"
      },
      "splash": {
        "image": "./assets/images/logo.png",
        "backgroundColor": "#0a0a0f",
        "resizeMode": "contain"
      },
      "permissions": [
        "CAMERA"
      ]
    },
    "plugins": [
      "expo-font",
      "expo-splash-screen",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow EventSchedulr to access your camera to scan QR codes."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}
